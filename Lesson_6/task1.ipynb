   "source": [
    "import random\n",
    "new_list = random.sample(range(0, 100), 10)\n",
    "i = len(new_list) - 1\n",
    "max_number = 0\n",
    "while i != -1:\n",
    "    if new_list[i] > max_number:\n",
    "        max_number = new_list[i]\n",
    "    i -= 1\n",
    "print(f'Список чисел: {new_list}\\nМаксимальное число: {max_number}')\n"
   ]
